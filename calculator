#include<stdio.h>
void main()
{
    int a,b;
    char op;
    printf("enter two numbers");
    scanf("%d%d",&a,&b);
    printf("enter symbol:");
    scanf(" %c",&op);
    switch(op)
    {
        case '+':printf("sum:%d",a+b);
                break;
        case '-':printf("diff:%d",a-b);
                break;
        case '*':printf("mul:%d",a*b);
                break;
        case '/':printf("div:%d",a/b);
                break;
        case '%':printf("mod div:%d",a%b);
                break;
        default:printf("wrong input");
        
    }
}
