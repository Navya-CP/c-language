#include<stdio.h>
int main()
{
    int a,b,c;
    printf("enter number1:");
    scanf("%d",&a);
    printf("enter number2:");
    scanf("%d",&b);
    printf("enter number3:");
    scanf("%d",&c);
    if(a>b && a>c)
    {
        if(b>c)
        printf("%d is second largest number",b);
        else
        printf("%d is second largest number",c);
    }
        else if(b>a && b>c)
        {
            if(a>c)
             printf("%d is second largest number",a);
             else
              printf("%d is second largest number",c);
        }
        else
        {
            if(a>b)
             printf("%d is second largest number",a);
             else
              printf("%d is second largest number",b);
        }
    }

    
