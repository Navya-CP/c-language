#include<stdio.h>
int main()
{
    int n;
    struct list
    {
        char name[20];
        int age;
        int reg;
        char grade;
    }
    detail[10];
    printf("how many details");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        printf("enter the name");
        scanf("%s",&detail[i].name);
        printf("enter the age\n");
        scanf("%d",&detail[i].age);
        printf("enter the reg\n");
        scanf("%d",&detail[i].reg);
        printf("enter the grade\n");
        scanf("%s",&detail[i].grade);
    }
    for(int i=0;i<n;i++)
    {
        printf("%s",detail[i].name);
        printf("%d",detail[i].age);
        printf("%d",detail[i].reg);
        printf("%c",detail[i].grade);
    }
    return 0 ;
}
